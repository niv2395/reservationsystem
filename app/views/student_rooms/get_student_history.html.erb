<% provide(:title, @student.name + '\'s rooms') %>

<h1><%="#{@student.name}'s"%> Booking History</h1>

<table class="table">
  <thead>
    <th>Room</th>
    <!--<th>Grade</th>-->
    <th>Status</th>
    <th></th>
  </thead>
  <tbody>
    <% @student_rooms.each do |student_room| %>
    <% if  student_room !=nil %>
      <tr>
        <td><%= student_room.room.title %></td>
        <!--<td><%= student_room.grade %></td>-->
        <td><%= student_room.status %></td>
        <td><%= link_to 'view', student_room %> </td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>


<br>
